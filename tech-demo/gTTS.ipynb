    "\n",
    "fp = open('input.txt', 'r')\n",
    "\n",
    "inputTxt = fp.read()\n",
    "\n",
    "language = 'ko'\n",
    "\n",
    "speech = gTTS(text = inputTxt, lang = language, slow = False)\n",
    "\n",
    "speech.save('hello.mp3')\n",
    "\n",
    "fp.close()\n"
